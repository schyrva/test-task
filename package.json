{
  "name": "luna-technical-interview",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:src": "eslint ./src --ext .js,.jsx,.ts,.tsx --ignore-pattern \"src/stories/**\" --ignore-pattern \"src/vite-env.d.ts\"",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:src:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix --ignore-pattern \"src/stories/**\" --ignore-pattern \"src/vite-env.d.ts\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "code:check": "yarn format && yarn lint:src",
    "code:fix": "yarn format && yarn lint:src:fix"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/vite": "^4.0.14",
    "axios": "^1.8.3",
    "classnames": "^2.5.1",
    "comments-removaljs": "^5.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.5",
    "@storybook/addon-a11y": "8.6.6",
    "@storybook/addon-essentials": "8.6.6",
    "@storybook/addon-links": "8.6.6",
    "@storybook/addon-onboarding": "8.6.6",
    "@storybook/blocks": "8.6.6",
    "@storybook/experimental-addon-test": "8.6.6",
    "@storybook/react": "8.6.6",
    "@storybook/react-vite": "8.6.6",
    "@storybook/test": "8.6.6",
    "@types/classnames": "^2.3.4",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/browser": "^3.0.8",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.9.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "playwright": "^1.51.0",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.5.3",
    "storybook": "8.6.6",
    "typescript": "^4.9.3",
    "typescript-eslint": "^8.26.1",
    "vite": "^4.1.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.0.8"
  }
}
